{ 
  "extends": "airbnb-base", 
  "rules": { 
    "no-unused-vars": 0, 
    "comma-dangle": 0, 
    "class-methods-use-this": 0, 
    "no-await-in-loop": 0, 
    "no-cond-assign": 0,
    "semi": 0,
    "no-restricted-syntax": 0,
    "no-underscore-dangle": 0
  }, 
  "globals": { 
    "use": true, 
    "$": true,
    "make": false 
  } 
}